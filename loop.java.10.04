public class Main {
  public static void main(String[] args) {
    int k = 1;

    while (k <=10) { //kordab seni kuni k = 10

      if (k % 2 == 0) {
        // liigub kaks sammu korraga 
        System.out.println(k + " is even");
      }
      else {
        System.out.println(k + " is odd");
      }
      k++;
    } 
  }


}

